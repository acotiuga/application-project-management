    <property>
      <code>#template('colorThemeInit.vm')

.pInfoLogo img {
float:left;
max-width:150px;
max-height:150px;
margin-right:10px;
}

#pContent {
padding-left: 10px;
padding-right: 10px;
overflow: hidden;
padding-top: 15px;
}

#pInfo1 {
 margin-top:10px;
 float: left;
 width: 45%;
 margin-bottom:10px;
}

#pInfo1 span{
 font-weight:normal;
}

#pInfo1 label{
 display: inline;
}

#pInfo2 {
 margin-top:10px;
 float: right;
 width: 45%;
 margin-bottom:10px;
}

#pInfo1-1, #pInfo1-2, #pInfo1-3 {
 float: left;
 clear: both;
 width: 45%;
}

#pInfo2-1, #pInfo2-2, #pInfo2-3 {
 float: right;
 width: 45%;
}

#pInfo1-1 li, #pInfo1-2 li, #pInfo1-3 li, #pInfo2-1 li, #pInfo2-2 li, #pInfo2-3 li{
 display:inline-block;
}

#pInfo3 {
 clear: both;
 width: 100%;
 padding: 5px;
}

#pInfo {
 overflow: auto;
 padding: 5px;
 padding-top: 0;
}

#pDocs1 {
 float: left;
 width: 49%;
}

#pDocs2 {
 float: right;
 width: 49%;
}

#pDocs3 {
 clear: both;
 width: 100%;
}

#pDocs1 select, #pDocs2 select, #pDocs3 select, #pInfo3 select {
 width: auto;
}

.addSubprojectLink {
display: block;
margin-top: 10px;
margin-bottom: 10px;
}</code>
